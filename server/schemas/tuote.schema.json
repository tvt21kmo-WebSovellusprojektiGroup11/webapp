{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "Nimi": "test product",
            "Kuvaus": "tst kuvaus ",
            "Kategoria": "a",
            "Hinta": 99.9,
            "Kuva": "https://dummyjson.com/image/i/products/1/3.jpg"
        }
    ],
    "required": [
        "Nimi",
        "Kuvaus",
        "Kategoria",
        "Hinta",
        "Kuva"
    ],
    "properties": {
        "Nimi": {
            "$id": "#/properties/Nimi",
            "type": "string",
            "title": "The Nimi schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "test product"
            ]
        },
        "Kuvaus": {
            "$id": "#/properties/Kuvaus",
            "type": "string",
            "title": "The Kuvaus schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "tst kuvaus "
            ]
        },
        "Kategoria": {
            "$id": "#/properties/Kategoria",
            "type": "string",
            "title": "The Kategoria schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "a"
            ]
        },
        "Hinta": {
            "$id": "#/properties/Hinta",
            "type": "number",
            "title": "The Hinta schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0.0,
            "examples": [
                99.9
            ]
        },
        "Kuva": {
            "$id": "#/properties/Kuva",
            "type": "string",
            "title": "The Kuva schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://dummyjson.com/image/i/products/1/3.jpg"
            ]
        }
    },
    "additionalProperties": true
}